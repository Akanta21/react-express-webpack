{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 6,
    "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "comma-dangle": 0,
    "global-require": 0,
    "no-console": 0,
    "object-curly-spacing": 0,
    "react/jsx-filename-extension": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./config/webpack.config.prod.js"
      }
    },
    "react": {
      "version": "15.4"
    }
  }
}
