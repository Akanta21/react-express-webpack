{
  "version": "1.0.0",
  "author": "Anton Fisher <a.fschr@gmail.com>",
  "engines": {
    "node": "6.9"
  },
  "scripts": {
    "dev": "NODE_ENV=development node ./src/server",
    "start": "NODE_ENV=production node ./build/server",
    "build:client": "mkdir -p ./build && rm -rf ./build/client && ./node_modules/.bin/webpack --progress --display-error-details --config=config/webpack.config.prod.js",
    "build:server": "mkdir -p ./build && rm -rf ./build/server && cp -r src/server build/server",
    "build": "npm run build:client && npm run build:server",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore --ignore-pattern node_modules -- .",
    "test": "exit 1"
  },
  "postcss": {},
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-watch": "2.0.6",
    "css-loader": "0.26.1",
    "eslint": "3.16.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "1.3.1",
    "react-hot-loader": "3.0.0-beta.6",
    "request": "2.79.0",
    "style-loader": "0.13.1"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "compression": "1.6.2",
    "express": "4.14.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.0"
  }
}
